function jhome {
  if [ -z $1 ]; then
    /usr/libexec/java_home -V
  else
    export JAVA_HOME=$(/usr/libexec/java_home --version $1)
  fi
}

function idea {
  open -na "IntelliJ IDEA.app" --args "$@"
}

function webstorm {
  open -na "webstorm.app" --args "$@"
}

function update {
  omz update
  brew update
  brew upgrade --greedy
  brew cleanup
}

